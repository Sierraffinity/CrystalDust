        if(defeated(TRAINER_RIVAL_GOLDENROD_TUNNEL_CHIKORITA) || defeated(TRAINER_RIVAL_GOLDENROD_TUNNEL_TOTODILE) || defeated(TRAINER_RIVAL_GOLDENROD_TUNNEL_CYNDAQUIL))
        {
