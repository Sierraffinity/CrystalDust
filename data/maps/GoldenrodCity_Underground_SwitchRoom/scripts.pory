script GoldenrodCity_Underground_SwitchRoom_EventScript_EmergencySwitch{
    lockall
    if(flag(FLAG_SWITCH_E_ON))
    {
        msgbox("It's labeled EMERGENCY.\pIt's ON.\nTurn it OFF?", MSGBOX_YESNO)
        if(var(VAR_RESULT) == NO)
        {
