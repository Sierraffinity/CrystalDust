        end
    }
    setvar(VAR_SHUTTERS, 7)
    setflag(FLAG_SWITCH_E_ON)
    setflag(FLAG_SWITCH_1_ON)
    setflag(FLAG_SWITCH_2_ON)
    setflag(FLAG_SWITCH_3_ON)
    goto(GoldenrodCity_Underground_SwitchRoom_EventScript_UpdateDoors)
    end
}

